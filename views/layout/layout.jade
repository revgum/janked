extends base

append head
  title #{blog.title}

block header
  div#header-wrapper
    div#header-left
      a.prominent(href="/") revgum.com
      &nbsp;
      a(href="/about") about
      &nbsp;
      a(href="/log") log
    div#header-right
      a(href="http://www.twitter.com/revgum") @revgum
      &nbsp;
      a(href="https://www.github.com/revgum") github/revgum

block footer
  div#footer-wrapper 
    span Inspired by 
    a(href="#") toto